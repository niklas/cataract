<% torrent = new_form %>
<% torrent.fetchable? %>
<%= error_messages_for :torrent, :object => torrent %>
<% form_for torrent do |f| %>
  URL: <%= f.text_field :url %>   <br/>
  Filename: <%= f.text_field :filename %>   <br/>
  
  <div id="messages_for_torrent"><%= torrent.fetchable_message %></div>

  <%= f.submit(torrent.fetchable? ? 'Fetch' : 'Create') %>
<% end %>

